{
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dw72/plupload/"
        },
        {
            "type": "package",
            "package": {
                "name": "codemirror/codemirror",
                "type": "library",
                "version": "3.20.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/codemirror/CodeMirror/",
                    "reference": "3.20.0"
                }
            }
        }
    ],
    "require": {
        "codemirror/codemirror": "^3.20",
        "iatspayments/php": "^2.1",
        "kigkonsult/icalcreator": "dev-master#e3dbec2cb3bb91a8bde989e467567ae8831a4026",
        "moxiecode/plupload": "dev-no-examples",
        "phpmailer/phpmailer": "<6.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "aliases": {
            "iatspayments/php": "iatspayments",
            "kigkonsult/icalcreator": "iCalcreator"
        },
        "patches": {
            "kigkonsult/icalcreator": {
                "Compatibility with 'composer_autoloader'": "https://www.drupal.org/files/issues/2018-03-28/composer-icalcreator-library-fix-2956818-3.patch"
            }
        }
    }
}
