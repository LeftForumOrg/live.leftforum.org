    "replace": {
        "drupal/drupal": "*"
    },
    "conflict": {
        "drupal/core": "8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/Libraries.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\Libraries::createSymlinks"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "DrupalProject\\composer\\ScriptHandler::removeInternalFiles"
        ]
    },
    "extra": {
        "merge-plugin": {
            "require": [
                "sites/*/libraries/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": false
        },
        "patches": {
            "cweagans/composer-patches": {
                "Call the preserve paths hooks": "https://github.com/SebCorbin/composer-patches/commit/1cb9bacba51f8bba0c5f3f322c3bb61e4ceae974.patch"
            },
            "drupal/commerce": {
                "Add a static cache and context to determine default currency": "https://drupal.org/files/issues/2657718-static_cache_default_currency-1.patch",
                "Make 'Add to cart' button text a display format setting (configurable at the field level by content type)": "https://drupal.org/files/issues/commerce_cart-button_text-1147690-75.patch",
                "Possibility to show price difference/total price in attributes Select List / Radio Buttons": "https://drupal.org/files/issues/product_select_options_token_display_alternative-1226466-36.patch"
            },
            "drupal/commerce_billy": {
                "Allow CSS styling to be programatically altered": "https://drupal.org/files/issues/2653972-billy-css-alter-2.patch"
            },
            "drupal/commerce_currency": {
                "Commerce Currency Field is not showing on Rules": "https://drupal.org/files/issues/2212615-field_visibility_for_rules-2.patch",
                "N/A": "https://drupal.org/files/issues/required_5.patch"
            },
            "drupal/commerce_discount": {
                "Product discounts: Taxes should applied to SUBTOTAL minus DISCOUNT": "https://www.drupal.org/files/issues/1962484-add-weight-commerce-discount-1.patch",
                "Multi Currency Support": "https://www.drupal.org/files/issues/2345311-commerce_discount_multicurrency-8.patch"
            },
            "drupal/ctools": {
                "Allow existing contexts to influence active context selection": "https://www.drupal.org/files/issues/2484541-context-dependency-1.patch"
            },
            "drupal/date": {
                "Allow a View Date Filter to Override the Timezone": "https://www.drupal.org/files/issues/date-views-filter-timezone-1820304-18.patch"
            },
            "drupal/date_ical": {
                "N/A": "https://www.drupal.org/files/issues/date_ical_remove_makefile-3.patch"
            },
            "drupal/entityreference": {
                "Field schema foreign keys is always hard-coded to 'node' despite target type not set to node entity type": "https://www.drupal.org/files/issues/1969018-update-field-config-foreign-keys-30.patch"
            },
            "drupal/field_collection": {
                "Undefined index: en in field_collection_field_attach_form()": "https://www.drupal.org/files/issues/undefined_langcode-1716526-15.patch"
            },
            "drupal/og_vocab": {
                "Allow all groups to be selected for vocabulary": "https://www.drupal.org/files/issues/2481153-og_vocab-accessible-groups-1.patch",
                "Allow relation get by group to pass back just vids": "https://www.drupal.org/files/issues/2604594-og_vocab_get_ogv_by_id-1.patch",
                "Fatal error when used with metatag in certain cases": "https://www.drupal.org/files/issues/fatal_error_with_metatag-2531418-1.patch"
            },
            "drupal/og": {
                "og_form_group_manager_validate() assumes the entity form has a language": "https://www.drupal.org/files/issues/group-manager-language-1865852.patch",
                "Add setting to auto-assign role to group manager broken on groups with overridden roles": "https://www.drupal.org/files/issues/og-default-role-member-2005800-40.patch",
                "Provide CTools relationship or context to get current group context": "https://www.drupal.org/files/issues/og_context_ctools_relationship-1781652-17.patch",
                "Infinite loop with og_context()": "https://www.drupal.org/files/issues/og_context_infinite_loop-2717489-10.patch",
                "N/A": "https://www.drupal.org/files/issues/og_id_user_access.patch",
                "og_ui will always cause content type named 'group' to use admin theme on node/add form": "https://www.drupal.org/files/og_ui-group_node_add_theme-1800208-5.patch"},
            "drupal/panelizer": {
                "Better Revision Handling inside and outside of Workbench Moderation": "https://www.drupal.org/files/issues/panelizer-n2457113-69.patch"
            },
            "drupal/pannels": {
                "Panelizer is incompatible with moderation": "https://www.drupal.org/files/issues/panelizer_is-1402860-82-fix-ipe-end-js-alert.patch"
            },
            "drupal/views": {
                "Add option to transform the case of term names": "https://www.drupal.org/files/issues/2408937-views-taxonomy-name-case-change-2.patch"
            },
            "drupal/views_argument_substitution": {
                "N/A": "https://www.drupal.org/files/issues/2650642-check-array-values-input-1.patch"
            }
        },
        "installer-paths": {
            "profiles/{$name}/": ["type:drupal-profile"],
            "sites/all/drush/{$name}/": ["type:drupal-drush"],
            "sites/all/libraries/{$name}/": ["type:drupal-library"],
            "sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
            "sites/all/themes/contrib/{$name}/": ["type:drupal-theme"]
